$(".graph-container").fadeOut(function(){
  $("#klass_chart").html("<%= escape_javascript(line_chart win_rates_constructeds_path(:klass_id => @klass), height: '200px' )%>");
  $("#klass-img").attr("src", "/assets/Icons/Classes/<%= Klass::LIST[@klass.to_i] %>_Icon.gif");
  $("#klass-name").text("<%= Klass::LIST[@klass.to_i] %>");
  $(".graph-container").fadeIn();
});
